@namespace("tests")
protocol SimpleProtocol {
    record Method {
        string name;
        string purpose;
    }

    record SamplePair {
        Method method;
        SampleNode node;
    }

    record SampleNode {
        int count = 0;
        array<SamplePair> subNodes;
    }

    record SelfRef {
        string something;
        array<SelfRef> subNodes = [];
    }

    SampleNode buildNodeTree(string declaration);
    SelfRef buildRefTree(string declaration);
}
